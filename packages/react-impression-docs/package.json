{
  "name": "react-impression",
  "version": "1.4.7",
  "author": "NewDadaFE",
  "description": "UI Components with React.",
  "main": "./dist/scripts/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/NewDadaFE/react-impression.git"
  },
  "homepage": "https://github.com/NewDadaFE/react-impression",
  "scripts": {
    "eslint": "eslint ./src/scripts/",
    "eslint:fix": "eslint ./src/scripts/ --fix",
    "start": "gulp build && webpack-dev-server --config webpack.dev.config.js --port 9008",
    "build:site": "gulp build && webpack -p --config webpack.prod.config.js",
    "scsslint": "scss-lint src/styles/**/*.scss",
    "clean": "rimraf ./dist && mkdir dist",
    "copy": "cp -a src/styles dist/styles",
    "compile:js": "node_modules/babel-cli/bin/babel.js src/scripts/components/impression --out-dir dist/scripts/",
    "compile:scss": "node-sass src/styles/index.scss dist/index.css",
    "compile:minscss": "node-sass --output-style compressed src/styles/index.scss dist/index.min.css",
    "prebuild": "npm run clean",
    "build": "npm run compile:js && npm run compile:scss && npm run compile:minscss",
    "postbuild": "npm run copy && gulp backup",
    "prepublish1": "npm config set registry https://registry.npmjs.org/",
    "postpublish1": "gulp recovery && npm config set registry https://registry.npm.taobao.org/"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "highlight.js": "^9.9.0",
    "moment": "^2.14.1",
    "perfect-scrollbar": "^0.6.12",
    "prop-types": "^15.5.10",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.4.1",
    "react-router": "3.0.5"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "eslint-config-react-impression": "^1.2.0",
    "eslint-loader": "^1.9.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-imagemin": "^3.0.3",
    "gulp-json-editor": "^2.2.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-run-sequence": "^0.3.2",
    "gulp-sass": "^3.1.0",
    "gulp-shell": "^0.6.3",
    "gulp-watch": "^4.3.9",
    "html-webpack-plugin": "^2.16.0",
    "is-primitive": "^2.0.0",
    "json-loader": "^0.5.4",
    "node-libs-browser": "^1.0.0",
    "node-sass": "^4.5.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
